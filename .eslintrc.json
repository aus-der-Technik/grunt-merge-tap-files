{
	"env": {
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"rules": {
		"no-unused-expressions": 1,
		"max-statements": 1,
		"no-with": 1,
		"wrap-iife": 1,
		"max-params": 1,
		"no-new-require": 2,
		"no-sync": 1,
		"no-undefined": 2,
		"no-delete-var": 2,
		"no-extra-semi": 1,
		"no-unmodified-loop-condition": 1,
		"array-callback-return": 2,
		"no-unsafe-finally": 1,
		"no-empty-character-class": 2,
		"no-label-var": 1,
		"no-func-assign": 2,
		"indent": 1,
		"no-regex-spaces": 1,
		"no-duplicate-case": 2,
		"no-irregular-whitespace": 0,
		"no-empty": 1,
		"no-obj-calls": 0,
		"func-name-matching": 0,
		"no-shadow-restricted-names": 2,
		"no-undef-init": 1,
		"no-dupe-keys": 2,
		"no-lone-blocks": 1,
		"eqeqeq": 1,
		"no-unused-labels": 2,
		"no-await-in-loop": 2,
		"no-extra-boolean-cast": 2,
		"no-new": 0,
		"no-redeclare": 1,
		"no-debugger": 2,
		"no-warning-comments": 1,
		"no-undef": 2,
		"no-unused-vars": 1,
		"no-magic-numbers": 0,
		"no-unsafe-negation": 2,
		"no-useless-return": 1,
		"no-new-wrappers": 0,
		"handle-callback-err": 1,
		"no-inner-declarations": 1,
		"no-duplicate-imports": 1,
		"no-implied-eval": 2,
		"require-jsdoc": 0,
		"consistent-return": 1,
		"consistent-this": 1,
		"semi": 1,
		"comma-dangle": 1,
		"radix": 1,
		"use-isnan": 2,
		"default-case": 1,
		"no-prototype-builtins": 1,
		"no-self-compare": 0,
		"no-constant-condition": 2,
		"no-use-before-define": 2,
		"no-template-curly-in-string": 2,
		"no-return-await": 1,
		"no-sparse-arrays": 2,
		"no-invalid-regexp": 2,
		"no-catch-shadow": 1,
		"callback-return": 1,
		"no-restricted-properties": 0,
		"no-ex-assign": 1,
		"no-lonely-if": 1,
		"no-throw-literal": 1,
		"no-extra-parens": 0,
		"no-dupe-args": 2,
		"no-useless-concat": 1,
		"no-fallthrough": 2,
		"max-nested-callbacks": 1,
		"func-names": 0,
		"max-lines": 0,
		"no-shadow": 1,
		"valid-jsdoc": 1,
		"valid-typeof": 2,
		"no-labels": 0,
		"no-empty-function": 1,
		"no-mixed-spaces-and-tabs": 1,
		"curly": 1,
		"no-cond-assign": 1,
		"no-unexpected-multiline": 1,
		"no-eval": 2,
		"no-unreachable": 2,
		"no-process-exit": 2,
		"no-self-assign": 0,
		"global-require": 0,
		"block-scoped-var": 2,
		"max-depth": 1,
		"no-return-assign": 1,
		"no-console": 1,
		"prefer-promise-reject-errors": 1,
		"no-param-reassign": 1
	}
}
